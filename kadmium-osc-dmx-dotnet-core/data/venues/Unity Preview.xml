<?xml version="1.0" encoding="UTF-8"?>
<venue xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="venue"
    xsi:schemaLocation="venue venue.xsd"
    xmlns:solver="solver">

  <universes>
    <universe name="Main Universe">
      <transmitters>
        <transmitter id="unityTransmitter"/>
      </transmitters>
      <fixtures>
        <fixtureSet src="Unity Still Washes.xml"/>
        <fixtureSet src="Unity Moving Washes.xml"/>
        <fixture type="Generic RGB" startChannel="480">
          <groups>
            <group name="guitarLEDs"/>
          </groups>
          <solvers>
            <solver:hsbSolver/>
            <solver:fakeUVSolver/>
            <solver:fakeStrobeSolver/>
          </solvers>
        </fixture>
      </fixtures>
    </universe>
    <universe name="Pixel Universe">
      <transmitters>
        <transmitter id="sacnTransmitter"/>
      </transmitters>
      <fixtures>
        <fixtureSet src="Unity FadeCandy.xml"/>
      </fixtures>
    </universe>
  </universes>
</venue>
