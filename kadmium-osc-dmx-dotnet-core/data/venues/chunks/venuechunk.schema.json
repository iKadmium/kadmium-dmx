{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Venue Chunk",
  "description": "Describes a venue chunk",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the venue"
    },
    "fixtures": {
      "type": "array",
      "description": "A list of universes for this venue",
      "items": {
        "type": "object",
        "properties": {
          "startChannel": {
            "description": "The channel assigned to the fixture",
            "type": "number"
          },
          "type": {
            "description": "The model name of the fixture",
            "type": "string"
          },
          "group": {
            "description": "The group the fixture is assigned to",
            "type": "string"
          },
          "options": {
            "description": "Options",
            "type": "array",
            "items": { "type": "string" }
          }
        }
      }
    }
  }
}
