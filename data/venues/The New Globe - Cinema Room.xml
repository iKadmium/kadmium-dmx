<?xml version="1.0" encoding="UTF-8"?>
<venue xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="venue.xsd"
       xmlns:xi="http://www.w3.org/2001/XInclude">

    <xi:include href="../groups.xml" />
    <xi:include href="../transmitters.xml" />

    <universes>
        <universe name="Main Universe">
            <transmitters>
                <transmitter id="sacnTransmitter"/>
                <transmitter id="enttecTransmitter"/>
            </transmitters>
            <fixtures>
                <fixture type="New Globe RGB LED" startChannel="1">
                    <groups>
                        <group name="vocalist"/>
                    </groups>
                    <solvers>
                        <solver type="hsb" enabled="true"/>
                        <solver type="fakeStrobe" enabled="true" countdownFrames="2"/>
                    </solvers>
                </fixture>

                <fixtureSet src="ColorRails.xml"/>

                <fixture type="New Globe RGB LED" startChannel="4">
                    <groups>
                        <group name="rearSides"/>
                    </groups>
                    <solvers>
                        <solver type="hsb" enabled="true"/>
                        <solver type="fakeStrobe" enabled="true" countdownFrames="2"/>
                    </solvers>
                </fixture>
            </fixtures>
        </universe>
    </universes>
</venue>
