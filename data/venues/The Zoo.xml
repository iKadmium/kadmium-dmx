<?xml version="1.0" encoding="UTF-8"?>
<venue xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="venue.xsd"
       xmlns:xi="http://www.w3.org/2001/XInclude">

    <xi:include href="../groups.xml" />
    <xi:include href="../transmitters.xml" />

    <universes>
        <universe name="Main Universe">
            <transmitters>
                <transmitter id="enttecTransmitter"/>
                <transmitter id="sacnTransmitter"/>
            </transmitters>
            <fixtures>
                <fixture startChannel="1" type="General BRWG Par">
                    <groups>
                        <group name="vocalist"/>
                    </groups>
                    <solver type="hsb" enabled="true"/>
                </fixture>

                <fixtureSet src="ColorRails.xml" />

                <fixture startChannel="21" type="General RGB Par">
                    <groups>
                        <group name="drummer"/>
                    </groups>
                    <solver type="hsb" enabled="true"/>
                    <solver type="masterAtFull" enabled="true"/>
                </fixture>

                <fixture startChannel="13" type="General RGBW Par">
                    <groups>
                        <group name="rearSides"/>
                    </groups>
                    <solver type="hsb" enabled="true"/>
                    <solver type="masterAtFull" enabled="true"/>
                </fixture>
            </fixtures>
        </universe>
    </universes>
</venue>
