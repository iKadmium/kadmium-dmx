        <listenerMap id="Main Universe" startIndex="1">
            <mapping address="/group/(.)/Hue" channel="Hue" />
            <mapping address="/group/(.)/Saturation" channel="Saturation" />
            <mapping address="/group/(.)/Brightness" channel="Brightness" />
            <mapping address="/group/(.)/Strobe" channel="Strobe" />

            <mapping address="/group/(.)/Chase" channel="Chase" />
            <mapping address="/group/(.)/Apeshit" channel="Apeshit" />

            <mapping address="/group/(.)/UV" channel="UV" />
            <mapping address="/group/(.)/Program" channel="Program" />

            <mapping address="/group/(.)/Pan" channel="Pan" />
            <mapping address="/group/(.)/Tilt" channel="Tilt" />
            <mapping address="/group/(.)/RandomMove" channel="RandomMove" />

            <mapping address="/group/(.)/OverrideEnabled" channel="OverrideEnabled" />
            <mapping address="/group/(.)/OverrideHue" channel="OverrideHue" />
            <mapping address="/group/(.)/OverrideSaturation" channel="OverrideSaturation" />
            <mapping address="/group/(.)/OverrideBrightness" channel="OverrideBrightness" />
        </listenerMap>
    </oscListener>
</listeners>