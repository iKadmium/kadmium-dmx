{
    "version": "0.1.0",
    "command": "dotnet",
    "isShellCommand": true,
    "tasks": [
        {
            "taskName": "build",
            "args": [
                "${workspaceRoot}\\kadmium-osc-dmx-dotnet-webui"
            ],
            "isBuildCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "test",
            "args": [
                "${workspaceRoot}\\kadmium-osc-dmx-dotnet-test"
            ],
            "isTestCommand": true,
            "problemMatcher": "$msCompile"
        },
        {
            "taskName": "publish",
            "args": [
                "${workspaceRoot}\\kadmium-osc-dmx-dotnet-webui\\kadmium-osc-dmx-dotnet-webui.csproj",
                "-c",
                "Release",
                "-r",
                "win10-x64",
                "-o",
                "${workspaceRoot}\\dist"
            ],
            "echoCommand": true
        },
        {
            "taskName": "install",
            "command": "npm",
            "options": {
                "cwd": "${workspaceRoot}\\kadmium-osc-dmx-dotnet-webui"
            },
            "args": [
                "install"
            ]
        },
        {
            "taskName": "serve",
            "options": {
                "cwd": "${workspaceRoot}\\kadmium-osc-dmx-dotnet-webui"
            },
            "command": "ng",
            "args": [
                "serve"
            ]
        }
    ]
}