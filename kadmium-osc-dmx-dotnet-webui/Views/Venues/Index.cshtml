@{
    Layout = "~/Views/Shared/_ListLayout.cshtml";
    ViewData["Title"] = "Venues";
    ViewData["Activate"] = true;

    var inputs = new InputElement[]
    {
        new InputText("name", "Name:"),
        new InputMultiSelect("fixtureCollections", "Fixture Collections:", FileAccess.GetFixtureCollectionNames().ToDictionary(x => x))
    };
}

@Html.Partial("ModalEdit", inputs)

<script src="~/js/Controllers/OSCListenerEdit.js"></script>