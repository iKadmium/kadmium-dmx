{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "moduleResolution": "classic",
    "outDir": "scripts/bin"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
